<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>diceServlet</servlet-name>
  	<display-name>diceServlet</display-name>
  	<description></description>
  	<servlet-class>dice.diceServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DiceServlet</servlet-name>
  	<display-name>DiceServlet</display-name>
  	<description></description>
  	<servlet-class>dice.DiceServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WeatherService</servlet-name>
  	<display-name>WeatherService</display-name>
  	<description></description>
  	<servlet-class>weather.WeatherService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MainWeather</servlet-name>
  	<display-name>MainWeather</display-name>
  	<description></description>
  	<servlet-class>weather.MainWeather</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>diceServlet</servlet-name>
  	<url-pattern>/diceServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DiceServlet</servlet-name>
  	<url-pattern>/DiceServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WeatherService</servlet-name>
  	<url-pattern>/WeatherService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MainWeather</servlet-name>
  	<url-pattern>/MainWeather</url-pattern>
  </servlet-mapping>
</web-app>
