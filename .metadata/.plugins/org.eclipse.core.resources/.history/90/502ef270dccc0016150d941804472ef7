package bitjava.day3;

public class Prob18
{
//- 없애는 건데 substring 사용하는 방법이랑 StringBuilder 클래스 이용하는 방법
	public static void main(String[] args)
	{
		String str="990102-1012345";
		
		//str=str.replaceAll(str.substring(str.indexOf("-")), "");
		str=str.replaceAll("-", "");
		
		System.out.print(str);
	}

}
