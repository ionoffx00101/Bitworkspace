package bitjava.day06;

import java.util.Scanner;

public class MeaningfulConst{
	public static void main(String[] args){
		System.out.print("숫자 입력");
		Scanner kb = new Scanner(System.in);
		
		int sel=kb.nextInt();
		
		switch (sel)
			{
			case week.MON:
				System.out.print("월요회의");
				break;
			case week.FRI:
				System.out.print("금요회의");
				break;
			case week.SUN:
				System.out.print("쉬는 날");
				break;

			case week.SAT:
				System.out.print("쉬는 날");
				break;

			default:
				break;
			}
	}
}
