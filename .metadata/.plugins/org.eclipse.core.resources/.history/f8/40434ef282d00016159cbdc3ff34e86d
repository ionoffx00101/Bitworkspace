package bitjava.day3;

import java.util.Scanner;

public class Prob15
{

	public static void main(String[] args)
	{
		String str = "Bit Bit Bit Bi t asdwelx well done crap";
		
		Scanner kb = new Scanner(System.in);
		System.out.print("문자열 1입력 : ");
		int strStart = kb.nextInt();
		Scanner kb2 = new Scanner(System.in);
		System.out.print("문자열 2입력 : ");
		int strStart2 = kb.nextInt();
		System.out.print(str.substring(strStart,strStart2+1));

	}

}
