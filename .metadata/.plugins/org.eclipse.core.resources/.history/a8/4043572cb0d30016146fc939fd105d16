package bitjava.day01_06;

public class ThreadEx01
{

	public static void main(String[] args)
	{

	}

}

class ThreadB extends Thread
{
	int total;

	@Override
	public void run()
	{
		synchronized (this)
		{
			System.out.println("B start");
			for (int i = 0; i < 100; i++)
			{
				total++;
				notify(); //자고 있는 쓰레드를 깨운다
			}

		}
	}
}