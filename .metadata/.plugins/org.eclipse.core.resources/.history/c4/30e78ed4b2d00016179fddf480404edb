package bitjava.day06;

import java.util.Scanner;

public interface MeaningfulConst
{
int MON=1,TUE=2,WED=3,THU=4,FRI=5,SAT=6,SUN=7;

}
public class MeaningfulConst{
	public static void main(String[] args){
		Scanner kb = new Scanner(System.in);
		
		int sel=kb.nextInt();
		
		switch (sel)
			{
			case MeaningfulConst.MON:
				System.out.print("월요회의");
				break;
			case MeaningfulConst.FRI:
				System.out.print("금요회의");
				break;
			case MeaningfulConst.SUN:
				System.out.print("쉬는 날");
				break;

			case MeaningfulConst.SAT:
				System.out.print("쉬는 날");
				break;

			default:
				break;
			}
	}
}
