package bitjava.day1;

public class SCE {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1=0;
		int num2 =2;
		boolean result;
		
		result =(num1+=10)<0 && (num2+=10)>0;
		System.out.print("result = "+result);
		System.out.print("num1="+num1+", num2="+num2);
		
		result =(num1+=10)<0 && (num2+=10)>0;
		System.out.print("result = "+result);
		System.out.print("num1="+num1+", num2="+num2);

	}

}
