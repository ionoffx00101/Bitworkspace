package bitjava.Prob;

public class Point
{
	int x = 0;
	int y = 0;

	public static void main(String[] args)
	{

	}

	public Point()
	{

	}

	public Point(int x, int y)
	{
		this.x = x;
		this.y = y;
	}

	public int getX()
	{
		return x;
	}

	public void setX(int x)
	{
		this.x = x;
	}

	public int getY()
	{
		return y;
	}

	public void setY(int y)
	{
		this.y = y;
	}

	public String toString()
	{
		return null;

	}

	public int[] getXY(){
		return [this.x,this.y];
	}
}
