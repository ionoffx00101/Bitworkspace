package bitjava.day01_05;

public class SyncObjectKey
{

	public static void main(String[] args)
	{

	}

}

class IHaveTwoNum
{
	int num1 = 0;
	int num2 = 0;

	public void addOneNum1()
	{
		num1++;
	}

	public void addTwoNum1()
	{
		num1 += 2;
	}

	public void addOneNum2()
	{
		num2++;
	}

	public void addTwoNum2()
	{
		num2 += 2;
	}

	public void showAllNums()
	{
		System.out.println(num1);
		System.out.println(num2);
	}
}

class AcessThread extends Thread
{
	IHaveTwoNum twoNuminst;

	public AcessThread(IHaveTwoNum inst)
	{
		twoNuminst = inst;
	}

	@Override
	public void run()
	{
		twoNuminst.addOneNum1();
		twoNuminst.addOneNum2();
		twoNuminst.addTwoNum1();
		twoNuminst.addTwoNum2();
	}
}