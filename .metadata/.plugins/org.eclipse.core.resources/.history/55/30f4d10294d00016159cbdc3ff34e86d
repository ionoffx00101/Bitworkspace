package bitjava.day06.Prob01;

public class book
{
	private String name;
	private Author author;
	private double price;
	private int qty;

	public static void main(String[] args)
	{
		Author a = new Author();
		book b = new book();
		a.setEmail("andne@ans.com");
		b.setPrice(200);
		b.setQty(2);
		

	}

	public void book(String name, Author author, double price, int qty)
	{
		this.name = name;
		this.author = author;
		this.price = price;
		this.qty = qty;

	}

	public double getPrice()
	{
		return price;
	}

	public void setPrice(double price)
	{
		this.price = price;
	}

	public int getQty()
	{
		return qty;
	}

	public void setQty(int qty)
	{
		this.qty = qty;
	}

	public String getName()
	{
		return name;
	}

	public Author getAuthor()
	{
		return author;
	}

	@Override
	public String toString()
	{
		return "'" + name + "' by" + author + "" + author.gender + "at" + author.email;
	}
}
