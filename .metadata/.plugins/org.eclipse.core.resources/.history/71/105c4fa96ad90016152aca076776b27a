package bitjava.day01_13;

import java.util.Scanner;

public class Prob01
{

	public static void main(String[] args)
	{
		Scanner kb = new Scanner(System.in);
		
		String munja = kb.nextLine();
		
		System.out.print(munja);
		System.out.println();
		char[] spmunja = null;
		
		for (int i = 0; i < munja.length(); i++)
		{
			spmunja[i]=munja.charAt(i+1);
		}
		
		for (int i = 0; i < spmunja.length; i++)
		{
			System.out.println(spmunja[i]);
		}
	}

}
