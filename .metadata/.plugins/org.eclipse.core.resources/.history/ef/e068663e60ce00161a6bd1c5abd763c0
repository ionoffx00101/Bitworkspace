package bookq.Q06;

public class Q06_04
{

	public static void main(String args[]) {
		Student s = new Student();
		s.name = " "; 홍길동
		s.ban = 1;
		s.no = 1;
		s.kor = 100;
		s.eng = 60;
		s.math = 76;
		System.out.println(" :"+s.name); 이름
		System.out.println(" :"+s.getTotal()); 총점
		System.out.println(" :"+s.getAverage()); 평균
		}
}

