package bitjava.day1;

public class bochug {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int result = 0;
		int count = 0;
		while (count < 99) {
			++count;
			result = count + result;
		}
		System.out.println(result);
		
		int count2=0;
		while(count2<=100){
			System.out.print(count2+" ");
		}
		
		
	}

}
