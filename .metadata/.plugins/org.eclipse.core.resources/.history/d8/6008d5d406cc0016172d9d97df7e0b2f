package bitjava.day2;

public class Prob06
{
	public static void main(String[] args)
	{
		recall(6);
	}

	public static int recall(int n)
	{
		if (n == 1)
		{
		//	System.out.println(((n-1)/2%3)==0 ? 1 :0);
			return n;
		} else
		{
			System.out.println(((n-1)/2%2)==0 ? 1 :0);
			return recall(n-1)/2;
		}

	}
}
