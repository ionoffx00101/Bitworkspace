package bitjava.day01_05;

public class RunableThread
{

	public static void main(String[] args)
	{

	}

}

class Sum
{
	int num;

	public Sum()
	{
		num = 0;
	}

	public int addNum(int n)
	{
		num += n;
	}

	public int getNum()
	{
		return num;
	}

}

class AdderThread extends Sum implements Runnable
{
	int start, end;

	public AdderThread(int start, int end)
	{
		this.start = start;
		this.end = end;
	}

	@Override
	public void run()
	{
	}

}