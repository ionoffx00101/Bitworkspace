package bitjava.day1;

public class Prob4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i = 0; i <= 100; i++) {
			for (int j = 0; j <= 100; j++) {
				System.out.println(i + "*" + j + "=" + i * j);
			}

		}
		for (int i = 0; i <= 100; i++) {
			
				System.out.println(1*i);
			

		}
	}

}
