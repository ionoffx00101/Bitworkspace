package bitjava;

public class UnicodeChar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch1 = 'A';
		char ch2 = 'C';
		char ch3 = 0x3091;
		char ch4 = 0x3092;
		
		System.out.print(ch1);
		System.out.print(ch2);
		System.out.print(ch3);
		System.out.print(ch4);
	}

}
